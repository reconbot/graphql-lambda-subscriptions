{
  "name": "graphql-lambda-subscriptions",
  "version": "0.0.0-development",
  "description": "Graphql-WS compatible Lambda Powered Subscriptions",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "engines": {
    "node": "^14.13 || >=16"
  },
  "repository": "git@github.com:reconbot/graphql-lambda-subscriptions.git",
  "homepage": "https://github.com/reconbot/graphql-lambda-subscriptions",
  "scripts": {
    "test": "mocha",
    "build": "npm run clean && tsc -p tsconfig-build.json && rollup -c && node -r esbuild-register bundle-types",
    "clean": "rm -rf dist-ts dist",
    "docs": "typedoc --options typedoc.js lib/index.ts",
    "format": "eslint lib/**/*.ts lib/*.ts --fix",
    "lint": "tsc && eslint lib/**/*.ts lib/*.ts",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "graphql",
    "lambda",
    "subscriptions",
    "API Gateway",
    "serverless",
    "subscriptionless",
    "AWS"
  ],
  "license": "MIT",
  "dependencies": {
    "debug": "^4.3.2",
    "streaming-iterables": "^7.0.0"
  },
  "peerDependencies": {
    "aws-sdk": ">= 2.0.0",
    "graphql": ">= 16.0.0"
  },
  "devDependencies": {
    "@architect/functions": "github:architect/functions#reconbot/expose-websocket-api",
    "@architect/sandbox": "5.5.4",
    "@graphql-tools/schema": "9.0.17",
    "@microsoft/api-extractor": "7.34.4",
    "@types/architect__sandbox": "3.3.3",
    "@types/aws-lambda": "8.10.111",
    "@types/chai": "4.3.4",
    "@types/chai-subset": "1.3.3",
    "@types/debug": "4.1.7",
    "@types/mocha": "10.0.1",
    "@types/node": "18.6.1",
    "@types/ws": "8.5.4",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "aggregate-error": "4.0.1",
    "aws-sdk": "2.1335.0",
    "chai": "4.3.7",
    "chai-subset": "1.6.0",
    "esbuild": "0.17.11",
    "esbuild-register": "3.4.2",
    "eslint": "8.36.0",
    "eslint-plugin-mocha-no-only": "1.1.1",
    "graphql": "16.6.0",
    "graphql-ws": "5.12.0",
    "inside-out-async": "1.0.1",
    "mocha": "10.2.0",
    "rollup": "3.19.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "semantic-release": "20.1.1",
    "typedoc": "0.23.26",
    "typedoc-plugin-markdown": "3.14.0",
    "typescript": "4.7.4",
    "ws": "8.13.0"
  },
  "mocha": {
    "bail": true,
    "timeout": "100s",
    "require": [
      "esbuild-register",
      "./lib/test/setup"
    ],
    "spec": "lib/**/*-test.ts"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
