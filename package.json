{
  "name": "graphql-lambda-subscriptions",
  "version": "0.0.0-development",
  "description": "Rollup your code and types for publishing",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index-esm.mjs"
  },
  "engines": {
    "node": "^14.13 || >=16"
  },
  "repository": "git@github.com:reconbot/graphql-lambda-subscriptions.git",
  "homepage": "https://github.com/reconbot/graphql-lambda-subscriptions",
  "scripts": {
    "test": "mocha",
    "build": "npm run clean && tsc -p tsconfig-build.json && rollup -c && node -r esbuild-register bundle-types",
    "clean": "rm -rf dist-ts dist",
    "docs": "typedoc --options typedoc.js lib/index.ts",
    "format": "eslint lib/**/*.ts lib/*.ts --fix",
    "lint": "tsc && eslint lib/**/*.ts lib/*.ts",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "graphql",
    "lambda",
    "subscriptions",
    "API Gateway",
    "serverless",
    "subscriptionless",
    "AWS"
  ],
  "license": "MIT",
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
    "@aws/dynamodb-expressions": "^0.7.3",
    "debug": "^4.3.2",
    "streaming-iterables": "^6.0.0"
  },
  "peerDependencies": {
    "aws-sdk": ">= 2.844.0",
    "graphql": ">= 14.0.0"
  },
  "devDependencies": {
    "@architect/functions": "^4.0.0",
    "@architect/sandbox": "git://github.com/reconbot/sandbox.git#17559f8d4296bc8cb5b43737d8357c7db803d6e2",
    "@graphql-tools/schema": "^8.0.3",
    "@microsoft/api-extractor": "^7.18.4",
    "@types/architect__sandbox": "^3.3.3",
    "@types/aws-lambda": "^8.10.81",
    "@types/chai": "^4.2.19",
    "@types/chai-subset": "^1.3.3",
    "@types/debug": "^4.1.7",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.6.0",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "aggregate-error": "^4.0.0",
    "aws-sdk": ">= 2.844.0",
    "chai": "^4.3.4",
    "chai-subset": "^1.6.0",
    "esbuild": "^0.12.20",
    "esbuild-register": "^3.0.0",
    "eslint": "^7.29.0",
    "graphql": ">= 14.0.0",
    "graphql-ws": "^5.3.0",
    "inside-out-async": "^1.0.0",
    "mocha": "^9.0.1",
    "rollup": "^2.56.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "semantic-release": "^17.4.4",
    "ts-node": "^10.2.0",
    "tslib": "^2.3.0",
    "typedoc": "^0.21.6",
    "typedoc-plugin-markdown": "^3.10.4",
    "typescript": "^4.3.5",
    "ws": "^8.0.0"
  },
  "mocha": {
    "bail": true,
    "timeout": "100s",
    "require": [
      "esbuild-register",
      "chai/register-assert",
      "./lib/test/setupPorts"
    ],
    "spec": "lib/**/*-test.ts"
  }
}
