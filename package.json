{
  "name": "graphql-lambda-subscriptions",
  "version": "0.0.0-development",
  "description": "Rollup your code and types for publishing",
  "types": "dist/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index-esm.mjs"
  },
  "engines": {
    "node": "^14.13 || >=16"
  },
  "repository": "git@github.com:reconbot/graphql-lambda-subscriptions.git",
  "homepage": "https://github.com/reconbot/graphql-lambda-subscriptions",
  "scripts": {
    "test": "mocha",
    "lint": "tsc && eslint lib/**/*.ts lib/*.ts",
    "format": "eslint lib/**/*.ts lib/*.ts --fix",
    "clean": "rm -rf dist-ts dist",
    "build": "npm run clean && tsc -p tsconfig-build.json && rollup -c && ts-eager bundle-types",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "graphql",
    "lambda",
    "subscriptions",
    "API Gateway",
    "serverless",
    "subscriptionless",
    "AWS"
  ],
  "license": "MIT",
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
    "@aws/dynamodb-expressions": "^0.7.3"
  },
  "peerDependencies": {
    "aws-sdk": ">= 2.844.0",
    "graphql": ">= 14.0.0"
  },
  "devDependencies": {
    "@architect/functions": "^4.0.0",
    "@architect/sandbox": "^4.0.1",
    "@graphql-tools/schema": "^8.0.3",
    "@microsoft/api-extractor": "^7.18.4",
    "@types/architect__sandbox": "^3.3.1",
    "@types/aws-lambda": "^8.10.81",
    "@types/chai": "^4.2.19",
    "@types/mocha": "^9.0.0",
    "@types/node": "^15.12.4",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "aws-sdk": ">= 2.844.0",
    "chai": "^4.3.4",
    "eslint": "^7.29.0",
    "graphql": ">= 14.0.0",
    "graphql-ws": "^5.3.0",
    "inside-out-async": "^1.0.0",
    "mocha": "^9.0.1",
    "rollup": "^2.56.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "semantic-release": "^17.4.4",
    "streaming-iterables": "^6.0.0",
    "ts-eager": "^2.0.2",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "ws": "^8.0.0"
  },
  "mocha": {
    "bail": true,
    "timeout": "100s",
    "require": [
      "ts-eager/register",
      "chai/register-assert"
    ],
    "spec": "lib/**/*-test.ts"
  }
}
